{
  "description": "Gulp.js tasks dedicated to the synchronization of translations with the Smartling service, based on the File API v1.",
  "homepage": "https://github.com/aquafadas-com/gulp-smartling",
  "name": "@aquafadas/gulp-smartling",

  "author": "Aquafadas <opensource@aquafadas.com>",
  "bugs": "https://github.com/aquafadas-com/gulp-smartling/issues",
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "repository": "aquafadas-com/gulp-smartling",
  "version": "0.3.0",

  "dependencies": {
    "smartling-sdk": "^0.2.2"
  },
  "devDependencies": {
    "@cedx/gulp-david": "^2.0.2",
    "codacy-coverage": "^2.0.0",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-istanbul": "^1.0.0",
    "gulp-jshint": "^2.0.1",
    "gulp-load-plugins": "^1.2.4",
    "gulp-mocha": "^3.0.1",
    "gulp-zip": "^3.2.0",
    "jsdoc": "^3.4.0",
    "jshint": "^2.9.2"
  },
  "engines": {
    "node": ">=6.3.0"
  },
  "jshintConfig": {
    "esversion": 6,
    "freeze": true,
    "futurehostile": true,
    "latedef": "nofunc",
    "mocha": true,
    "noarg": true,
    "nocomma": true,
    "node": true,
    "nonbsp": true,
    "nonew": true,
    "singleGroups": true,
    "trailing": true,
    "typed": true,
    "undef": true,
    "unused": true,
    "varstmt": true
  },
  "keywords": [
    "gulp",
    "gulpplugin",
    "i18n",
    "smartling",
    "translation"
  ],
  "scripts": {
    "coverage": "codacy-coverage < var/lcov.info",
    "test": "gulp test"
  }
}
